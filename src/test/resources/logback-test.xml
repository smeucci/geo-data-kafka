<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="O" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<charset>UTF-8</charset>
			<pattern>%d{"yyyy-MM-dd HH:mm:ss,SSS"} %-5p [%c{0}] %m%n</pattern>
		</encoder>
	</appender>

	<root level="INFO">
		<appender-ref ref="O" />
	</root>

</configuration>